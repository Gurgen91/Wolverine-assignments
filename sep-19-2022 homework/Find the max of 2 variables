#include <iostream>

int main() {
    int n1 = 0;
    int n2 = 0;
    std::cout << "Enter the first number please: ";
    std::cin >> n1;
    std::cout << "Enter the second number please: ";
    std::cin >> n2;
  
    if (n1 < n2) {std::cout << "Maximum = " << n2 << std::endl;} 
    else {std::cout << "Maximum = " << n1 << std::endl;}
    return 0;
}
